<!-- Le fonts -->
<style>
  @font-face {
    font-family: 'proxima-nova';
    src: url('<%= font_url('ProximaNovaLight.eot') %>');
    src: url('<%= font_url('ProximaNovaLight.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= font_url('ProximaNovaLight.woff') %>') format('woff'),
         url('<%= font_url('ProximaNovaLight.ttf') %>') format('truetype');
    font-weight:300;
    font-style:normal;
  }
   
  @font-face {
    font-family: 'proxima-nova';
    src: url('<%= font_url('ProximaNovaRegular.eot') %>');
